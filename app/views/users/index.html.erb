<div id="photos" class="transitions-enabled">
	<% @users.each do |user| %>

	<div class="box">
		<div class="panel panel-default">
			<% if user.photos.any? %>
				<%= link_to image_tag(user.photos.first.image.url(:medium)), user %><br/>
			<% end %>

			<div class="panel-body">
	    		<strong>Photographer:</strong>
	    		<%= link_to user.email, user %><br/>
    		</div>
  		</div>
	</div>
	
	<% end %>
</div>
<br>